doctype 5
html
    head
        title= title
        if cssIsCompiled
            link(rel='stylesheet', href = cssCompiled)
        else
            link(rel='stylesheet', href = cssBasic)

        if jsIsCompiled
            script(src = jsCompiled)
            script(src = bootstrap)
        else
            script(src = goog)
            script(src = deps)
            script()
                goog.require('app');
            script(src = bootstrap)
            block head

    body.expr_body
        .container-fluid
            block intro
            .row-fluid
                .span8
                    block content
                .span3.offset1
                    block syslog

        block bottomScripts