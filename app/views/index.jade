extends layout

block bottomScripts

    if jsIsCompiled
        script(src = jsCompiled)
    else
        script(src = goog)
        script(src = deps)
        script()
            goog.require('app');

    script(src = bootstrap)
    script()
        bootstrap.setDefaults('#{wsServer}', '#{wsPort}', '#{landing}');
        bootstrap.addApp('site');

    script(src='https://api.tiles.mapbox.com/mapbox.js/v1.6.3/mapbox.js')
    link(href='https://api.tiles.mapbox.com/mapbox.js/v1.6.3/mapbox.css', rel='stylesheet')
    script(src = "http://maps.google.com/maps/api/js?sensor=false")