extends layout

block bottomScripts
    if jsIsCompiled
            script(src = jsCompiled)
        else
            script(src = goog)
            script(src = deps)
            script()
                goog.require('app');

    script(src = bootstrap)
    script()
        bootstrap.setDefaults('#{wsServer}', '#{wsPort}', '#{landing}');
        bootstrap.addApp('site');

